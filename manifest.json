{
    "manifest_version": 2,
    "name": "Crypt Keeper",
    "version": "1.0",
    "description": "Stores your passwords from all webpages you consent to.",
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_popup": "/popup.html",
        "default_icon": "/browser-icon.svg"
    },
    "background": {
        "scripts": ["background.js"]
    }
}